exportClasses("Document", "DocumentPage")
exportClasses("BoundingBox", "TextBoundingBox", "ShapeBoundingBox")

export(getNumPages, getTextBBox, getPages)

exportMethods(getNumPages)


S3method(dim, Document)

export(margins)
exportMethods(margins)

export(left, right, top, bottom, width, height)
exportMethods(left, right, top, bottom)


export(findLineBreaks)

if(FALSE) {
S3method(margins, DocumentPage)
S3method(margins, Document)
S3method(margins, TextBoundingBox)
}

